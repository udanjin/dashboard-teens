{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
    },
    "include": ["src/**/*", "src/utils", "src/index.ts"],
    "exclude": ["node_modules","dist"],
    "paths": {
      "@/*": ["./src/*"]
    }
  }